{
  "manifest_version": 3,
  "name": "Toranoana Downloader",
  "description": "downloader for toranoana online hondana",
  "version": "1.0",
  "action": {
    "default_icon": "icons/icon128.png",
    "default_popup": "dist/index.html"
  },
  "permissions": [
    "downloads",
    "storage",
    "tabs"
  ],
  "host_permissions": [
  ],
  "content_scripts": [
    {
      "matches": [
        "*://hentaiverse.org/*"
      ],
      "js": [
        "dist/content.js"
      ]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "service_worker": "dist/background.js"
  }
}
